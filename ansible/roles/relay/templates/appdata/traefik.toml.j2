[global]
    checkNewVersion = false
    sendAnonymousUsage = false
    defaultEntryPoints = ["http"]

[log]
    logLevel = "WARN"

[accessLog]

[entrypoints]
    [entrypoints.http]
        address = ":80"

        [entryPoints.http.forwardedHeaders]
            trustedIPs = ["{{ automate_ip }}"]

[retry]

[api]
    dashboard = true

[docker]
    endpoint = "unix:///var/run/docker.sock"
    watch = true
    exposedbydefault = false
