[global]
    checkNewVersion = false
    sendAnonymousUsage = false
    defaultEntryPoints = ["http"]

[log]
    logLevel = "INFO"

[accessLog]

[entrypoints]
    [entrypoints.http]
        address = ":80"

[retry]

[api]
    dashboard = true

[docker]
    endpoint = "unix:///var/run/docker.sock"
    watch = true
    exposedbydefault = false
